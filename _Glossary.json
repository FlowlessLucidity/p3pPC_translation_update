        "Id": 266,
        "English": "Стрега"
    },
    {
        "Id": 267,
        "English": "Группа из трёх человек,"
    },
    {
        "Id": 268,
        "English": "умеющих призывать Персон."
    },
    {
        "Id": 269,
        "English": "Вам удалось задержать"
    },
    {
        "Id": 270,
        "English": "Чидори - одну из них."
    },
    {
        "Id": 271,
        "English": "Однако её мотивы"
    },
    {
        "Id": 272,
        "English": "остаются загадкой."
    },
    {
        "Id": 273,
        "English": "Стрега"
    },
    {
        "Id": 274,
        "English": "Группа из трёх человек,"
    },
    {
        "Id": 275,
        "English": "призывающих Персон."
    },
    {
        "Id": 276,
        "English": "У них особое представление"
    },
    {
        "Id": 277,
        "English": "о жизни и смерти, отличное"
    },
    {
        "Id": 278,
        "English": "от представления"
    },
    {
        "Id": 279,
        "English": "обычных людей."
    },
    {
        "Id": 280,
        "English": "Стрега"
    },
    {
        "Id": 281,
        "English": "Группа из трёх"
    },
    {
        "Id": 282,
        "English": "враждебных пользователей"
    },
    {
        "Id": 283,
        "English": "Персон. Одна из них,"
    },
    {
        "Id": 284,
        "English": "Чидори, пожертвовала"
    },
    {
        "Id": 285,
        "English": "собой, чтобы спасти"
    },
    {
        "Id": 286,
        "English": "жизнь Джумпею."
    },
    {
        "Id": 287,
        "English": "Супрессант"
    },
    {
        "Id": 288,
        "English": "Лекарство, которое"
    },
    {
        "Id": 289,
        "English": "Синдзиро получает от"
    },
    {
        "Id": 290,
        "English": "Стреги, чтобы подавить"
    },
    {
        "Id": 291,
        "English": "его Персону. Побочные"
    },
    {
        "Id": 292,
        "English": "эффекты могут быть"
    },
    {
        "Id": 293,
        "English": "смертельными."
    },
    {
        "Id": 294,
        "English": "Инцидент (2 г. назад)"
    },
    {
        "Id": 295,
        "English": "Во время борьбы с"
    },
    {
        "Id": 296,
        "English": "заблудшей Тенью Синдзиро"
    },
    {
        "Id": 297,
        "English": "потерял контроль над"
    },
    {
        "Id": 298,
        "English": "своей Персоной, что"
    },
    {
        "Id": 299,
        "English": "привело к гибели"
    },
    {
        "Id": 300,
        "English": "матери Кена."
    },
    {
        "Id": 301,
        "English": "Смерть"
    },
    {
        "Id": 302,
        "English": "Высшее существо,"
    },
    {
        "Id": 303,
        "English": "которое Икуцуки"
    },
    {
        "Id": 304,
        "English": "пытался создать."
    },
    {
        "Id": 305,
        "English": "Оно должно было"
    },
    {
        "Id": 306,
        "English": "принести Конец света."
    },
    {
        "Id": 308,
        "English": "Смерть"
    },
    {
        "Id": 309,
        "English": "Истинная сущность"
    },
    {
        "Id": 310,
        "English": "Рёдзи Мотизуки."
    },
    {
        "Id": 311,
        "English": "Могущественная"
    },
    {
        "Id": 312,
        "English": "Тень с силой"
    },
    {
        "Id": 313,
        "English": "XIII аркана."
    },
    {
        "Id": 315,
        "English": "Никта"
    },
    {
        "Id": 316,
        "English": "Мать всех Теней,"
    },
    {
        "Id": 317,
        "English": "с древнейших времён"
    },
    {
        "Id": 318,
        "English": "приносящая смерть."
    },
    {
        "Id": 319,
        "English": "Если она пробудится,"
    },
    {
        "Id": 320,
        "English": "то случится настоящий"
    },
    {
        "Id": 321,
        "English": "Конец света."
    },
    {
        "Id": 322,
        "English": "Канун Нового года"
    },
    {
        "Id": 323,
        "English": "День, в который вы"
    },
    {
        "Id": 324,
        "English": "должны дать ответ Рёдзи."
    },
    {
        "Id": 325,
        "English": "Либо вы убьёте его, либо"
    },
    {
        "Id": 326,
        "English": "забудете всё то, что"
    },
    {
        "Id": 327,
        "English": "знаете о надвигающемся"
    },
    {
        "Id": 328,
        "English": "Конце света."
    },
    {
        "Id": 329,
        "English": "31 января"
    },
    {
        "Id": 330,
        "English": "День, в который Никта"
    },
    {
        "Id": 331,
        "English": "низойдёт в мир и"
    },
    {
        "Id": 332,
        "English": "принесёт ему смерть."
    },
    {
        "Id": 333,
        "English": "На вершине Тартара"
    },
    {
        "Id": 334,
        "English": "вы сможете столкнуться"
    },
    {
        "Id": 335,
        "English": "с Никтой лицом к лицу."
    },
    {
        "Id": 336,
        "English": "Стрега"
    },
    {
        "Id": 337,
        "English": "пользователей Персоны,"
    },
    {
        "Id": 338,
        "English": "будет вам противостоять."
    },
    {
        "Id": 339,
        "English": "В обещанный день вы"
    },
    {
        "Id": 340,
        "English": "встретите Джина и Такая."
    },
    {
        "Id": 341,
        "English": "Чидори чудесным образом"
    },
    {
        "Id": 342,
        "English": "вернулась к жизни."
    },
    {
        "Id": 343,
        "English": "Выпускной"
    },
    {
        "Id": 344,
        "English": "Вы с друзьями пообещали"
    },
    {
        "Id": 345,
        "English": "друг другу встретиться"
    },
    {
        "Id": 346,
        "English": "в день выпускного, хоть"
    },
    {
        "Id": 347,
        "English": "и память обо всём"
    },
    {
        "Id": 348,
        "English": "случившемся исчезнет"
    }
]